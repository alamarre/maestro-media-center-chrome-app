{
    "name": "Maestro Media Center",
    "description": "An HTML5 media center that automatically plays next episode / season and supports Chromecast.",
    "version": "0.2.1",
    "manifest_version": 2,
    "icons": {
        "16": "maestro-16.png",
        "128": "maestro-128.png"
    },
    "permissions": [
        {
            "socket": [
                "tcp-connect",
                "tcp-listen"
            ]
        },
        {
            "system": [
                "network"
            ]
        },
        {
            "fileSystem": [
                "directory",
                "retainEntries"
            ]
        },
        "storage"
    ],
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    }
}